<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>Xiaowang0229sJsonLibrary</PackageId>
    <Title>Xiaowang0229's JsonLibrary</Title>
    <Authors>Xiaowang0229</Authors>
    <RepositoryType>git</RepositoryType>
    <Version>1.0.0</Version>
    <Company>Xiaowang's Software Developoment Group.</Company>
    <Description>Json操作(更简易的读写)</Description>
    <RepositoryUrl>https://github.com/Xiaowang0229/JsonLibrary</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
  </ItemGroup>

</Project>
